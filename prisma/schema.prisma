generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Calls {
  id              Int      @id @default(autoincrement())
  remoteCallId    String   @unique @map("remote_call_id")
  from            String   
  to              String
  startedAt       DateTime @db.Timestamptz()  @map("started_at")
  endedAt         DateTime? @db.Timestamptz()  @map("ended_at")
  durationInMs    Int?      @map("duration_in_ms")
//reconciledAt    DateTime @db.Timestamptz()  @map("reconciled_at")

  @@map("calls")
}